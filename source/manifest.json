{
 	"manifest_version": 3,
	"name": "GeoGuessr Image  Extraction",
	"permissions": ["scripting", "activeTab", "webRequest"],
	"host_permissions": ["https://www.geoguessr.com/*", "https://streetviewpixels-pa.googleapis.com/v1/*"],
 	"description": "Extracts image from a street view in GeoGuessr",
  	"author" : "Dmytro Avdieienko & Daniil Zahoruiko",
	"version": "1.0.0",
  	"action": {
    		"default_popup": "index.html",
    		"default_icon": "./images/icon.webp"
  	},
	"background": {
			"service_worker": "./scripts/background.js"
	}
}
